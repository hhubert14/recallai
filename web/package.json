{
    "name": "recallai",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "stripe": "stripe listen --forward-to localhost:3000/api/v1/stripe/webhook",
        "test": "vitest",
        "test:unit": "vitest --run --testNamePattern=unit",
        "test:integration": "vitest --run --testNamePattern=integration",
        "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
        "db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
        "db:push": "dotenv -e .env.local -- drizzle-kit push",
        "db:push:test": "dotenv -e .env.test.local -- drizzle-kit push",
        "db:pull": "dotenv -e .env.local -- drizzle-kit pull",
        "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
        "db:studio:test": "dotenv -e .env.test.local -- drizzle-kit studio",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:status": "docker-compose ps"
    },
    "dependencies": {
        "@langchain/core": "^0.3.57",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@stripe/stripe-js": "^7.3.1",
        "@supabase/ssr": "^0.6.1",
        "@supabase/supabase-js": "^2.49.8",
        "@types/stripe": "^8.0.417",
        "@vercel/analytics": "^1.5.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "dotenv": "^17.2.3",
        "drizzle-orm": "^0.44.6",
        "langchain": "^0.3.27",
        "lucide-react": "^0.511.0",
        "next": "^16.0.7",
        "next-themes": "^0.4.6",
        "openai": "^5.1.0",
        "postgres": "^3.4.7",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "stripe": "^19.1.0",
        "tailwind-merge": "^3.3.0",
        "youtube-transcript-api": "^2.0.4"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/chrome": "^0.0.326",
        "@types/node": "^20.19.0",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@types/youtube": "^0.1.2",
        "@vitejs/plugin-react": "^5.0.2",
        "dotenv-cli": "^10.0.0",
        "drizzle-kit": "^0.31.5",
        "eslint": "^9",
        "eslint-config-next": "^16.0.7",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.0",
        "jsdom": "^26.1.0",
        "prettier": "^3.5.3",
        "tailwindcss": "^4.1.7",
        "tsx": "^4.20.6",
        "tw-animate-css": "^1.3.0",
        "typescript": "^5.9.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    }
}
